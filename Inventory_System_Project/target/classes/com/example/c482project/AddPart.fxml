<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="addPartForm" prefHeight="653.0" prefWidth="754.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.c482project.AddPartController">
    <Label fx:id="addPartLabel" layoutX="43.0" layoutY="64.0" text="Add Part Form">
        <font>
            <Font name="JetBrains Mono Bold" size="20.0" />
        </font>
    </Label>
    <RadioButton fx:id="addPartInHouseRadioButton" layoutX="271.0" layoutY="68.0" mnemonicParsing="false" onAction="#addPartInHouseRadioButtonClicked" text="In-House Part">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
        <toggleGroup>
            <ToggleGroup fx:id="addPartToggleGroup" />
        </toggleGroup>
    </RadioButton>
    <RadioButton fx:id="addPartOutSourcedRadioButton" layoutX="505.0" layoutY="68.0" mnemonicParsing="false" onAction="#addPartOutSourcedRadioButtonClicked" text="Outsourced Part" toggleGroup="$addPartToggleGroup">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </RadioButton>
    <TextField fx:id="addPartIDTextField" disable="true" editable="false" layoutX="289.0" layoutY="152.0" opacity="0.5" prefHeight="29.0" prefWidth="160.0" text="Auto Generated">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </TextField>
    <TextField fx:id="addPartNameTextField" layoutX="289.0" layoutY="201.0" />
    <TextField fx:id="addPartPriceTextField" layoutX="289.0" layoutY="250.0" />
    <TextField fx:id="addPartInvTextField" layoutX="289.0" layoutY="297.0" />
    <TextField fx:id="addPartMaxTextField" layoutX="289.0" layoutY="342.0" />
    <TextField fx:id="addPartMinTextField" layoutX="289.0" layoutY="389.0" />
    <TextField fx:id="machineIdOrCompName" layoutX="289.0" layoutY="430.0" />
    <Label fx:id="addPartIdLabel" layoutX="50.0" layoutY="156.0" text="Part ID">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="addPartNameLabel" layoutX="50.0" layoutY="204.0" text="Part Name">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="addPartInvLevelLabel" layoutX="50.0" layoutY="300.0" text="Inventory">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="addPartPriceLabel" layoutX="51.0" layoutY="253.0" text="Part Price/Cost">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="addPartMaxLabel" layoutX="54.0" layoutY="345.0" text="MAX">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="machineCompanyIDLabel" layoutX="55.0" layoutY="433.0" text="Machine ID">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Label fx:id="addPartMinLabel" layoutX="54.0" layoutY="392.0" text="MIN">
        <font>
            <Font name="JetBrains Mono Bold" size="14.0" />
        </font>
    </Label>
    <Button fx:id="addPartSaveButton" layoutX="478.0" layoutY="521.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save">
        <font>
            <Font name="JetBrains Mono Regular" size="14.0" />
        </font>
    </Button>
    <Button fx:id="addPartCancelButton" layoutX="556.0" layoutY="521.0" mnemonicParsing="false" onAction="#addPartCancelButtonClicked" text="Cancel">
        <font>
            <Font name="JetBrains Mono Regular" size="14.0" />
        </font>
    </Button>
</AnchorPane>
